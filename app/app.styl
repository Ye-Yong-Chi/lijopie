.wrapper
  font-family: "儷黑 Pro", "sans-serif", "LiHei Pro", "微軟正黑體", "新細明體";

#header
  -webkit-border-radius: 0px
  -moz-border-radius: 0px
  border-radius: 0px
  background-color: #009A93

#header .item
  font-size: 1rem

#title_logo img
  height: 1rem
  display: inline

.cover
  background-image: url(res/cover.svg)
  -webkit-background-size: cover
  -moz-background-size: cover
  -o-background-size: cover
  background-size: cover
  color: #fff
  font-weight: bold 

.our-centered
  text-align: center
